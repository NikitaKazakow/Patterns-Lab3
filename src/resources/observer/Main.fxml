<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<StackPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="observer.FaceController">
   <ImageView fx:id="background" fitHeight="300.0" fitWidth="300.0" pickOnBounds="true" preserveRatio="true" />
    <ImageView fx:id="leftEye" fitHeight="75.0" fitWidth="75.0" layoutX="50.0" layoutY="50.0" pickOnBounds="true" preserveRatio="true" StackPane.alignment="TOP_LEFT">
      <StackPane.margin>
         <Insets left="100.0" top="150.0" />
      </StackPane.margin></ImageView>
   <ImageView fx:id="rightEye" fitHeight="75.0" fitWidth="75.0" pickOnBounds="true" preserveRatio="true" StackPane.alignment="TOP_RIGHT">
      <StackPane.margin>
         <Insets right="100.0" top="150.0" />
      </StackPane.margin></ImageView>
   <ImageView fx:id="mouth" fitHeight="50.0" fitWidth="150.0" pickOnBounds="true" preserveRatio="true" StackPane.alignment="BOTTOM_CENTER">
      <StackPane.margin>
         <Insets bottom="100.0" />
      </StackPane.margin></ImageView>
</StackPane>
